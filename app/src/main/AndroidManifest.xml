<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gankedbyhousing">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".myListings"
            android:screenOrientation="nosensor"/>
        <activity android:name=".ViewListingsActivity"
            android:screenOrientation="nosensor"/>
        <activity android:name=".MakeListings"
            android:screenOrientation="nosensor"/>
        <activity android:name=".updatePhoneActivity"
            android:screenOrientation="nosensor"/>
        <activity android:name=".updateLocationActivity"
            android:screenOrientation="nosensor"/>
        <activity android:name=".ProfileActivity"
            android:screenOrientation="nosensor"/>
        <activity android:name=".SignUpActivity"
            android:screenOrientation="nosensor"/>
        <activity android:name=".MainActivity"
            android:screenOrientation="nosensor"/>
        <activity android:name=".LoginActivity"
            android:screenOrientation="nosensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>